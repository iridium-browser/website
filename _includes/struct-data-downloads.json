	[
		{
		"@context": "http://schema.org/",
		"@type": "SoftwareApplication",
		"name": "{{ site.title }}",
		"url": "{{ site.url }}",
		"downloadUrl": "{{ page.url | prepend: site.baseurl | prepend: site.url }}",
		"description": "{{ site.description | strip_newlines }}",
		"applicationCategory": "Browser",
		"operatingSystem": [ "{{ page.structured-data.os1 }}"{% if page.structured-data.os2 %}, "{{ page.structured-data.os2 }}"{% else %}{% endif %}{% if page.structured-data.os3 %}, "{{ page.structured-data.os3 }}"{% else %}{% endif %}{% if page.structured-data.os4 %}, "{{ page.structured-data.os4 }}"{% else %}{% endif %}{% if page.structured-data.os5 %}, "{{ page.structured-data.os5 }}"{% else %}{% endif %}{% if page.structured-data.os6 %}, "{{ page.structured-data.os6 }}"{% else %}{% endif %}{% if page.structured-data.os7 %}, "{{ page.structured-data.os7 }}"{% else %}{% endif %}
			], 
		"AggregateRating": {
			"@type": "AggregateRating",
			"ratingValue": "{{ page.structured-data.value }}",
			"bestRating" : "5",
			"worstRating": "1",
			"ratingCount": "{{ page.structured-data.count }}"
			},
			"image": {
				"@type": "ImageObject",
				"height": "200",
				"width": "200",
				"contentUrl": "{{ site.hotlogo | absolute_url }}",
				"url": "{{ site.url | append: site.baseurl }}"
			},
			"offers": {
				"@type": "Offer",
				"description": "free",
				"priceCurrency": "EUR",
				"price": "0.00"		
			}
		}, 
		{
		"@context": "http://schema.org",
		"@type": "WebPage",
		"name": "{{ page.title }}",
		"url": "{{ page.url | prepend: site.baseurl | prepend: site.url }}",
		"description": "{{ page.description }}"
		},
		{
		"@context": "http://schema.org",
		"@type": "BreadcrumbList",
		"itemListElement": [
			{
			"@type": "ListItem",
			"position": 1,
			"item": {
				"@id": "{{ site.url | append: site.baseurl }}",
				"name": "home"
				}
			},
			{
			"@type": "ListItem",
			"position": 2,
			"item": {
				"@id": "{{ '/downloads/' | absolute_url }}",
				"name": "Download"
				}
			},
			{
			"@type": "ListItem",
			"position": 3,
			"item": {
				"@id": "{{ page.url | absolute_url }}",
				"name": "{{ page.title }}"
				}
			}
			]
		}
	]